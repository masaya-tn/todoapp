.container
  = form_with(model: board, local: true) do |f|
    %div
      = f.label :title, 'name'
    %div
      = f.text_field :title, class: "title_form"
    %div
      = f.label :content, 'Description'
    %div
      = f.text_area :content, class: "content_form"
    = f.submit 'submit', class: 'btn-primary'
      
  
    